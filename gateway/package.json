{
  "name": "gateway",
  "version": "1.0.0",
  "main": "./index.js",
  "private": true,
  "bin": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "prod": "node ./dist/index.js",
    "dev": "babel-node ./src/index.js",
    "start": "node ./out/index.js",
    "watch": "nodemon --watch src --exec babel-node ./src/index.js",
    "test": "NODE_ENV=test mocha --recursive test/unit --require ./test/helpers/start-server",
    "synk": "snyk test",
    "build": "babel src -d out",
    "build:webpack-dev": "webpack --env=development",
    "build:webpack-prod": "webpack --env=production",
    "lint": "eslint ./src",
    "lint-test": "npm run lint && npm run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-test"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@google-cloud/logging-winston": "^0.11.1",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.4.7",
    "body-parser": "^1.19.0",
    "colors": "^1.1.2",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "deepmerge": "^3.2.0",
    "dotenv": "^4.0.0",
    "esther": "^1.0.8",
    "express": "^4.16.0",
    "grpc": "^1.20.0",
    "grpc-utils": "^1.0.14",
    "helmet": "^3.9.0",
    "horeb": "^1.1.2",
    "http-proxy": "^1.17.0",
    "kubernetes-client": "^6.11.0",
    "lodash": "^4.17.14",
    "morgan": "^1.9.0",
    "node-fetch": "^2.6.1",
    "path-to-regexp": "^3.0.0",
    "promise": "^8.0.1",
    "protobufjs": "^6.8.8",
    "redis": "^2.8.0",
    "swagger-fluent": "^3.1.2",
    "validator": "^11.1.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.2.0",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.0.3",
    "dockerode": "^2.5.8",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-flowtype": "^3.9.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^2.7.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.7",
    "sinon": "^7.3.2",
    "snyk": "^1.134.2",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-node-externals": "^1.7.2"
  },
  "pkg": {
    "scripts": "src/controllers/**/*.js",
    "assets": "locales/**/*"
  }
}
